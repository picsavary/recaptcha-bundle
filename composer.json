{
    "name": "amps/recaptcha-bundle",
    "description": "RecaptchaBundle for my web sites",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "anne marie savary",
            "email": "picsavary@icloud.com"
        }
    ],
    "require": {
        "php": "^8.1.4",
        "symfony/config": "^6.1",
        "symfony/form": "6.0.*",
        "symfony/dependency-injection": "6.0.*",
        "symfony/http-kernel": "6.0.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "6.0.*",

        "symfony/browser-kit": "6.0.*",
        "symfony/yaml": "6.0.*",
        "google/recaptcha": "^1.2.4"
    },
    "autoload": {
        "psr-4": {
            "Amps\\RecaptchaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amps\\RecaptchaBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
