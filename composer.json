{
    "name": "amps/recaptcha-bundle",
    "description": "RecaptchaBundle for my web sites",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "anne marie savary",
            "email": "picsavary@icloud.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/config": "5.0.*",
        "symfony/form": "5.0.*",
        "symfony/dependency-injection": "5.0.*",
        "symfony/http-kernel": "5.0.*"

    },
    "require-dev": {
        "symfony/phpunit-bridge": "5.0.*",

        "symfony/browser-kit": "5.0.*",
        "symfony/yaml": "5.0.*",
        "google/recaptcha": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Amps\\RecaptchaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amps\\RecaptchaBundle\\Tests\\": "tests/"
        }
    }
}
